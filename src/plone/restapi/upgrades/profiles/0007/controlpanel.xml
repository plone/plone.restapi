<?xml version="1.0"?>
<object
  name="portal_controlpanel"
  xmlns:i18n="http://xml.zope.org/namespaces/i18n"
  i18n:domain="plone.restapi">

  <configlet
    title="Slots"
    action_id="slots_settings"
    appId="plone.restapi.slots"
    category="Plone"
    condition_expr=""
